					<matchconditions>
						<matchcondition type="referencedby" table="TModuleSelector">
							<matchconditions>
								<matchcondition type="property" property="ControllerName">DamageController</matchcondition>
							</matchconditions>
						</matchcondition>
					</matchconditions>
				</reference>
			</change>
			<change operation="set" property="Default">
				<reference table="TModernWarfareDamageModuleDescriptor">
					<matchconditions>
						<matchcondition type="property" property="__createdby">FIN Zeds Nostovaki HPD</matchcondition>
					</matchconditions>
				</reference>
			</change>
		</changes>
	</ndfpatch>
	
